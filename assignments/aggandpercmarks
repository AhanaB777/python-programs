subject1=int(input("Enter marks of first subject: "))
subject2=int(input("Enter marks of second subject: "))
subject3=int(input("Enter marks of third subject: "))
subject4=int(input("Enter marks of fourth subject: "))
subject5=int(input("Enter marks of fifth subject: "))

agg_marks=subject1+subject2+subject3+subject4+subject5
print(f"Aggregate marks of student is {agg_marks} ")

perc_marks=(agg_marks/500)*100
print(f"Percentage marks of student is {perc_marks} ")
